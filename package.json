{
    "name": "pinia-store-decorators",
    "version": "1.0.2",
    "description": "Typescript Decorators to define Pinia stores.",
    "repository": "https://github.com/Zolyn/pinia-store-decorators.git",
    "license": "MIT",
    "author": "Zorin <zorin1477@gmail.com>",
    "exports": {
        ".": {
            "import": "./dist/pinia-store-decorators.es.js",
            "require": "./dist/pinia-store-decorators.cjs.js"
        }
    },
    "main": "./dist/pinia-store-decorators.cjs.js",
    "module": "./dist/pinia-store-decorators.es.js",
    "types": "./dist/pinia-store-decorators.d.ts",
    "typings": "./dist/pinia-store-decorators.d.ts",
    "files": [
        "dist/pinia-store-decorators.*.js",
        "dist/pinia-store-decorators.*.js.map",
        "dist/pinia-store-decorators.d.ts"
    ],
    "scripts": {
        "clean": "rm -rf ./dist",
        "build": "vite build && tsc",
        "api": "api-extractor run",
        "format": "prettier --write ./src/**/*.ts ./src/*.ts",
        "prepare": "husky install"
    },
    "dependencies": {
        "reflect-metadata": "^0.1.13"
    },
    "devDependencies": {
        "@commitlint/cli": "12.1.4",
        "@commitlint/config-conventional": "12.1.4",
        "@microsoft/api-extractor": "7.18.16",
        "@release-it/conventional-changelog": "3.3.0",
        "@typescript-eslint/eslint-plugin": "5.1.0",
        "@typescript-eslint/parser": "5.1.0",
        "eslint": "7.27.0",
        "eslint-config-alloy": "4.4.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-prettier": "3.4.1",
        "husky": "6.0.0",
        "lint-staged": "11.0.0",
        "prettier": "2.3.0",
        "release-it": "14.11.6",
        "sort-package-json": "1.50.0",
        "typescript": "4.4.4",
        "vite": "2.6.10"
    },
    "peerDependencies": {
        "pinia": "^2.0.0-rc.14",
        "vue": "^3.2.20"
    }
}
